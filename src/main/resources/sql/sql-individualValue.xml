<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="individualValue">
	<select id="selectList" parameterType="com.kdw.pokeivemul.IndividualValue.vo.IndividualValueVo" resultType="com.kdw.pokeivemul.IndividualValue.vo.IndividualValueVo">
		/* individualValue.selectList */
		SELECT
			SEQ
			, NUMBER
			, NAME
			, H
			, A
			, B
			, C
			, D
			, S
		FROM
			TB_INDIVIDUAL_VALUE
		ORDER BY
			NUMBER ASC, NAME ASC
    </select>
</mapper>