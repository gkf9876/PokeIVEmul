<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="baseStats">
	<select id="selectList" parameterType="com.kdw.pokeivemul.baseStats.vo.BaseStatsVo" resultType="com.kdw.pokeivemul.baseStats.vo.BaseStatsVo">
		/* baseStats.selectList */
		SELECT
			SEQ
			, NUMBER
			, NAME
			, H
			, A
			, B
			, C
			, D
			, S
		FROM
			TB_BASE_STATS
		ORDER BY
			NUMBER ASC, NAME ASC
    </select>
</mapper>